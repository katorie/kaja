.block
  .secondary-navigation
    %ul.wat-cf
      %li.first=link_to pat(:list), url(:communities, :index)
      %li.active=link_to pat(:new), url(:communities, :new)
  .content
    %h2.title
      =pat(:new)
      =mt(:community)
    .inner
      -form_for :community, url(:communities, :create), class: :form do |f|
        =partial "communities/form", locals: { f: f }

-content_for :sidebar, partial("base/sidebar")
